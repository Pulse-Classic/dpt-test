						<AbsInset left="11" right="12" top="12" bottom="11" />
					</BackgroundInsets>
					<TileSize val="32" />
					<EdgeSize val="32" />
				</Backdrop>
				<Frames>
					<Slider inherits="HybridScrollBarTemplate" minValue="0" maxValue="0">
						<Size x="20" y="171" />
						<Anchors>
							<Anchor point="TOPLEFT" x="539" y="-14" />
						</Anchors>
					</Slider>
				</Frames>
			</Frame>
			<Frame name="PD_NewRaidFrame" parentKey="PD_MainFrame">
