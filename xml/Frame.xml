					<BackgroundInsets>
						<AbsInset left="11" right="12" top="12" bottom="11" />
					</BackgroundInsets>
					<TileSize val="32" />
					<EdgeSize val="32" />
				</Backdrop>
				<Layers>
					<Layer level="OVERLAY">
						<FontString inherits="GameFontNormal" text="Pulse Dkp">
							<Size x="80" y="20" />
							<Anchors>
								<Anchor point="TOPLEFT" x="13" y="-23" />
							</Anchors>
							<Color r="0.502" g="0.502" b="1" />
						</FontString>
					</Layer>
				</Layers>
				<Frames>
					<Button name="PD_Close" inherits="UIPanelButtonTemplate" text="X">
						<Size x="20" y="20" />
						<Anchors>
							<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" />
						</Anchors>
						<Scripts>
							<OnClick>
								PD_MainFrame_Close();
							</OnClick>
						</Scripts>
					</Button>
				</Frames>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad>
				PD_MainFrame_OnLoad()
			</OnLoad>
		</Scripts>
	</Frame>
</Ui>