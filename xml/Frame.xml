				<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
					<BackgroundInsets>
						<AbsInset left="11" right="12" top="12" bottom="11" />
					</BackgroundInsets>
					<TileSize val="32" />
					<EdgeSize val="32" />
				</Backdrop>
				<Layers>
					<Layer level="OVERLAY">
						<FontString inherits="GameFontNormal" text="Raid name:" justifyH="LEFT">
							<Size x="80" y="20" />
							<Anchors>
								<Anchor point="TOPLEFT" x="19" y="-22" />
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
				<Frames>
					<Button name="PD_NewRaidButton" inherits="UIPanelButtonTemplate" text="Create new raid">
						<Size x="87" y="23" />
						<Anchors>
							<Anchor point="TOPLEFT" x="96" y="-77" />
						</Anchors>
						<Scripts>
							<OnClick>
								PD_CreateRaid();
							</OnClick>
						</Scripts>
					</Button>
					<EditBox inherits="InputBoxTemplate" autoFocus="false" name="PD_NewRaidName">
						<Size x="163" y="32" />
						<Anchors>
							<Anchor point="TOPLEFT" x="23" y="-43" />
						</Anchors>
					</EditBox>
				</Frames>
				<Scripts>
					<OnLoad>
					</OnLoad>
				</Scripts>
			</Frame>
			<Frame name="PD_CurrentRaidFrame" parentKey="PD_MainFrame">
				<Size x="382" y="188" />
				<Anchors>
					<Anchor point="TOPLEFT" x="186" y="-64" />
