							<Size x="80" y="20" />
							<Anchors>
								<Anchor point="TOPLEFT" x="21" y="-107" />
							</Anchors>
						</FontString>
					</Layer>
				</Layers>
				<Frames>
					<Button inherits="UIPanelButtonTemplate" text="End raid">
						<Size x="75" y="23" />
						<Anchors>
							<Anchor point="TOPLEFT" x="276" y="-133" />
						</Anchors>
					</Button>
				</Frames>
			</Frame>
			<Frame name="PD_TitleFrame" parentKey="PD_MainFrame">
